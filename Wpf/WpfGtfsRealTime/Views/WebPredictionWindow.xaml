<controls:MetroWindow x:Class="WpfGtfsRealTime.Views.WebPredictionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfGtfsRealTime.Views"
        xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:wpfBrowser="clr-namespace:WpfBrowser;assembly=WpfBrowser"
        mc:Ignorable="d"
        Title="WebPredictionWindow" Height="600" Width="800">
    <!--Target="{Binding Path=TargetUri, Mode=TwoWay}"-->
    <Grid>
        <wpfBrowser:WebBrowser 
        Target="{Binding Path=WebPredictionUrlForStop, Mode=OneWay}"
        ShowAddressBar="True"
        ShowNavigationButtons="True" /> 
    </Grid>
</controls:MetroWindow>
